    <div class="wrapper">
      <!-- #BeginLibraryItem "/library/ur_here.lbi" --><!-- #EndLibraryItem -->
    </div>


  <div class="container wrapper">
    <h2>搜索“{$search_keywords|escape}”的产品</h2>
        <div class="list-box">
            <ul class="prod-list cl">
              <!--{foreach from=$goods_list item=goods}-->
                <!-- {if $goods.goods_id} -->
                 <li class="prod-item">
                       <a target="_blank" href="{$goods.url}">
                       <div class="figure">
                       <img src="{$goods.goods_thumb}" alt="{$goods.goods_name}" class="goodsimg" />
                        </div>
                        <h3 title="Y35L">{$goods.goods_name}</h3>
                        <p class="price"><dfn>&yen;</dfn>
                        <!--{if $goods.promote_price neq "" } -->
                        {$goods.promote_price}
                        <!--{else}-->
                        {$goods.shop_price}
                        <!--{/if}-->
                        </p>
                  </li>
                <!--{/if}-->
              <!--{/foreach}-->
            </ul>
        </div>

    <!-- #BeginLibraryItem "/library/pages.lbi" --><!-- #EndLibraryItem -->

  </div>
</div>


